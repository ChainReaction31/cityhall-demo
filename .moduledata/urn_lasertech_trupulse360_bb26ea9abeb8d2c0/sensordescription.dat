<sml:PhysicalSystem xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:sml="http://www.opengis.net/sensorml/2.0" xmlns:swe="http://www.opengis.net/swe/2.0" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:gco="http://www.isotc211.org/2005/gco" xmlns:gmd="http://www.isotc211.org/2005/gmd" gml:id="TRUPULSE_BB26EA9ABEB8D2C0">
  <gml:description>Laser RangeFinder for determining distance, inclination, and azimuth</gml:description>
  <gml:identifier codeSpace="uid">urn:lasertech:trupulse360:bb26ea9abeb8d2c0</gml:identifier>
  <gml:name>TruPulse Range Finder [Ian [HTC-M8]]</gml:name>
  <sml:validTime>
    <gml:TimePeriod gml:id="T1">
      <gml:beginPosition>2018-01-25T17:10:27.881Z</gml:beginPosition>
      <gml:endPosition indeterminatePosition="now"/>
    </gml:TimePeriod>
  </sml:validTime>
  <sml:outputs>
    <sml:OutputList>
      <sml:output name="rangeData">
        <swe:DataStream>
          <swe:elementType name="rangeData">
            <swe:DataRecord definition="http://sensorml.com/ont/swe/property/LaserRangeData">
              <swe:field name="time">
                <swe:Time definition="http://www.opengis.net/def/property/OGC/0/SamplingTime" referenceFrame="http://www.opengis.net/def/trs/BIPM/0/UTC">
                  <swe:label>Sampling Time</swe:label>
                  <swe:uom xlink:href="http://www.opengis.net/def/uom/ISO-8601/0/Gregorian"/>
                </swe:Time>
              </swe:field>
              <swe:field name="horizDistance">
                <swe:Quantity definition="http://sensorml.com/ont/swe/property/HorizontalDistance">
                  <swe:label>Horizontal Distance</swe:label>
                  <swe:uom code="m"/>
                </swe:Quantity>
              </swe:field>
              <swe:field name="slopeDistance">
                <swe:Quantity definition="http://sensorml.com/ont/swe/property/LineOfSightDistance">
                  <swe:label>Line-of-Sight Distance</swe:label>
                  <swe:uom code="m"/>
                </swe:Quantity>
              </swe:field>
              <swe:field name="azimuth">
                <swe:Quantity definition="http://sensorml.com/ont/swe/property/TrueHeading" referenceFrame="http://sensorml.com/ont/swe/property/NED" axisID="z">
                  <swe:label>True Heading</swe:label>
                  <swe:uom code="deg"/>
                </swe:Quantity>
              </swe:field>
              <swe:field name="inclination">
                <swe:Quantity definition="http://sensorml.com/ont/swe/property/Inclination" referenceFrame="http://sensorml.com/ont/swe/property/NED" axisID="y">
                  <swe:label>Inclination</swe:label>
                  <swe:uom code="deg"/>
                </swe:Quantity>
              </swe:field>
            </swe:DataRecord>
          </swe:elementType>
          <swe:encoding>
            <swe:TextEncoding collapseWhiteSpaces="true" decimalSeparator="." tokenSeparator="," blockSeparator="&#xa;"/>
          </swe:encoding>
          <swe:values/>
        </swe:DataStream>
      </sml:output>
    </sml:OutputList>
  </sml:outputs>
</sml:PhysicalSystem>